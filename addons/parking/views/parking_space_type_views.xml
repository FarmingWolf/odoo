<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <record id="parking_space_type_action" model="ir.actions.act_window">
        <field name="name">车位类型</field>
        <field name="res_model">parking.space.type</field>
        <field name="view_mode">tree,form</field>
        <field name="help" type="html">
            <p class="o_view_nocontent_smiling_face">
                根据需要创建车位类型。
            </p>
        </field>
    </record>

    <record id="parking_space_type_view_tree" model="ir.ui.view">
        <field name="name">parking.space.type.tree</field>
        <field name="model">parking.space.type</field>
        <field name="arch" type="xml">
            <tree string="parking_space_type_tree">
                <field name="sequence" widget="handle"/>
                <field name="name"/>
                <field name="space_cnt"/>
            </tree>
        </field>
    </record>

    <record id="parking_space_type_view_form" model="ir.ui.view">
        <field name="name">parking.space.type.form</field>
        <field name="model">parking.space.type</field>
        <field name="arch" type="xml">
            <form string="parking_space_type_form">
                <group col="3">
                    <group>
                        <field name="name"/>
                    </group>
                    <group>
                        <field name="sequence"/>
                    </group>
                    <group>
                        <field name="space_cnt" readonly="1"/>
                    </group>
                </group>
            </form>
        </field>
    </record>
</odoo>
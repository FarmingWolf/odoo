<?xml version="1.0" encoding="utf-8"?>
<odoo>

        <data noupdate="1">
            <record id="base.module_category_website_estate" model="ir.module.category">
                <field name="name">estate slide</field>
                <field name="description">在线招商房源管理</field>
                <field name="sequence">37</field>
            </record>

            <record id="group_website_estate_officer" model="res.groups">
                <field name="name">Officer</field>
                <field name="category_id" ref="base.module_category_website_estate"/>
                <field name="implied_ids" eval="[(4, ref('website.group_website_restricted_editor'))]"/>
            </record>

            <record id="group_website_estate_manager" model="res.groups">
                <field name="name">Manager</field>
                <field name="category_id" ref="base.module_category_website_estate"/>
                <field name="implied_ids" eval="[(4, ref('group_website_estate_officer'))]"/>
                <field name="users" eval="[(4, ref('base.user_root')), (4, ref('base.user_admin'))]"/>
            </record>

        </data>
</odoo>

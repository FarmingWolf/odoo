<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <record id="estate_lease_contract_party_a_unit_action" model="ir.actions.act_window">
        <field name="name">资产租赁合同甲方单位设置</field>
        <field name="res_model">estate.lease.contract.party.a.unit</field>
        <field name="view_mode">tree,form</field>
        <field name="help" type="html">
            <p class="o_view_nocontent_smiling_face">
                按需设置资产租赁合同用甲方单位。
            </p>
        </field>
    </record>
    <record id="estate_lease_contract_party_a_unit_tree" model="ir.ui.view">
        <field name="name">estate.lease.contract.party.a.unit.tree</field>
        <field name="model">estate.lease.contract.party.a.unit</field>
        <field name="arch" type="xml">
            <tree string="estate_lease_contract_party_a_unit">
                <field name="party_a_unit_id"/>
                <field name="sequence" widget="handle"/>
            </tree>
        </field>
    </record>
    <record id="estate_lease_contract_party_a_unit_form" model="ir.ui.view">
        <field name="name">estate.lease.contract.party.a.unit.form</field>
        <field name="model">estate.lease.contract.party.a.unit</field>
        <field name="arch" type="xml">
            <form string="estate_lease_contract_party_a_unit_form">
                <sheet>
                    <group>
                        <field name="party_a_unit_id"/>
                    </group>
                </sheet>
            </form>
        </field>
    </record>
</odoo>